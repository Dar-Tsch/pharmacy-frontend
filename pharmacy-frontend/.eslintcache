harmacy-frontend\\src\\PagesO\\Contacts.js",["58"],"import React, {Component} from 'react';\r\nimport ReactDOM from 'react-dom'\r\nimport {Container, Form, Button} from \"react-bootstrap\";\r\n\r\nexport default class Contacts extends Component {\r\n\r\n    state = {\r\n        text: '',\r\n        text2: '',\r\n\r\n        select: ''\r\n    }\r\n\r\n\r\n    change = (e) => {\r\n        const {name, value} = e.target\r\n        this.setState({[name]: value})\r\n\r\n    }\r\n\r\n\r\n    handleClick() {\r\n        console.log(\"Надіслано\")\r\n    }\r\n\r\n    render() {\r\n        const {text, text2, select} = this.state\r\n        return (\r\n\r\n            <Container style={{width:'500px'}}>\r\n                <h1 className=\"text-center\">Зв'яжіться з нами</h1>\r\n                <Form onSubmit={this.submit} >\r\n                    <h6>Ім'я</h6>\r\n                    <input name=\"text\" value={text} onChange={this.change}/>\r\n                    <h6>Прізвище</h6>\r\n                    <input name=\"text2\" value={text2} onChange={this.change}/>\r\n\r\n                    <select name=\"select\" value={select} onChange={this.change} className=\"m-3\">\r\n\r\n                        <option value=\"1\">До 18 років</option>\r\n                        <option value=\"2\">18-25 років</option>\r\n                        <option value=\"3\">25-40 років</option>\r\n                        <option value=\"4\">Більше 40 років</option>\r\n\r\n                    </select>\r\n\r\n\r\n\r\n                    <Form.Group controlId=\"formBasicEmail\">\r\n                        <Form.Label>Email-адреса</Form.Label>\r\n                        <Form.Control type=\"email\" placeholder=\"Введіть email\"/>\r\n                        <Form.Text>\r\n                            Ми ніколи не відправимо ваш email ще комусь\r\n                        </Form.Text>\r\n                    </Form.Group>\r\n\r\n                    <Form.Group controlId=\"formBasicPassword\">\r\n                        <Form.Label>Ваше повідомлення:</Form.Label>\r\n                        <Form.Control as=\"textarea\" rows=\"3\"/>\r\n                    </Form.Group>\r\n\r\n                    <input type=\"hidden\" name=\"project_name\" value=\"Site Name\"/>\r\n                    <input type=\"hidden\" name=\"admin_email\" value=\"daschatschernenko@gmail.com\"/>\r\n                    <input type=\"hidden\" name=\"form_subject\" value=\"Form Subject\"/>\r\n\r\n                    <Button variant=\"primary\" type=\"submit\" onClick={this.handleClick.bind(this)}>Надіслати</Button>\r\n\r\n\r\n                </Form>\r\n\r\n\r\n            </Container>\r\n        )\r\n    }\r\n    componentDidUpdate() {\r\n        console.log(this.state)\r\n    }\r\n}\r\n\r\n\r\n\r\n","C:\\Users\\DarTsch\\Desktop\\kursova\\pharmacy-frontend\\src\\PagesO\\Home.js",["59","60"],"import React, {Component} from 'react';\r\nimport CarouselBox from \"../ComponentsO/CarouselBox\";\r\nimport {Button, Card, CardDeck, Col, Container, Row} from \"react-bootstrap\";\r\nimport rosl from '../assets/rosl.jpg';\r\nimport Jumbotron from \"../ComponentsO/Jubotron\";\r\nimport Footer from '../ComponentsO/Footer';\r\n\r\nclass Home extends Component {\r\n    render() {\r\n        return (<>\r\n                <CarouselBox/>\r\n                <Container>\r\n                    <h2 className=\"text-center m-4\">Аптека</h2>\r\n                    <CardDeck>\r\n                        <Card>\r\n                            <Card.Img\r\n                                variant=\"top\"\r\n                                src=\"https://cdnimg.rg.ru/img/content/139/69/20/Depositphotos_40474311_xl-2015_d_850.jpg\"\r\n                            />\r\n                            <Card.Body>\r\n                                <Card.Title>Доставка ліків по Україні</Card.Title>\r\n                                <Card.Text>\r\n                                    Під час епідемії коронавірусу залишайтеся вдома й замовляйт